<?xml version='1.0' encoding='UTF-8'?>
<flow-definition plugin="workflow-job@1.0">
    <keepDependencies>false</keepDependencies>
    <properties>
        <com.synopsys.arc.jenkinsci.plugins.jobrestrictions.jobs.JobRestrictionProperty plugin="job-restrictions@0.3"/>
    </properties>
    <definition class="org.jenkinsci.plugins.workflow.cps.CpsFlowDefinition" plugin="workflow-cps@1.0">
        <script>checkout changelog: true, 
                		 poll: true, 
                		 scm: [$class: GitSCM,
                		       branches: [[name: */%s]], 
                       		   doGenerateSubmoduleConfigurations: false, 
                       		   extensions: [], 
                       		   submoduleCfg: [], 
                       		   userRemoteConfigs: [[credentialsId: a07018ed-216c-4606-9568-957d97dd59a9,
                        							url: %s]]
                     		   ]
		</script>
        <sandbox>false</sandbox>
    </definition>
    <triggers/>
</flow-definition>